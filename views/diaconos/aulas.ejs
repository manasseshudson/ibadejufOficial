            <div class="container-fluid">
                <div class="row">
					<h2 style="margin: 15px;">Matérias Disponíveis</h2>
					<% materia.forEach(materias=>{ %>
						<a href="/user/aulas_materia/<%= materias.id_materia %>/<%= uid_usuario %>">
							<div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
								<div class="white-box analytics-info-cs mg-b-10 res-mg-b-30 res-mg-t-30 table-mg-t-pro-n tb-sm-res-d-n dk-res-t-d-n" style="border: 2px solid #006DF0;border-radius: 13px;">
									<h3 class="box-title"></h3>
									
									<ul class="list-inline two-part-sp">
										<li>
											<img src="/img/materias.png"/ style="width: 90px;height: 90px;">
										</li>
										<li class="text-right sp-cn-r" style="margin-left: 15px;width: 50%;">
											<%= materias.descricao %>
										</li>
									</ul>
								</div>                       
							</div>
						</a>
                    <% }) %>
                </div>
            </div>
