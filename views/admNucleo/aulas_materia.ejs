                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="product-payment-inner-st">
							<!--<form role="form" action="/adm/addAluno" method="post" onsubmit="validar()">-->
							<form id="formulario" action="javascript:func()" method="post">
								<input type="hidden" name="uid_usuario" id="uid_usuario" value="<%= uid_usuario %>" >
								<div class="row">
									<div class="col-md-2 px-1" style="display:none;">
										<div class="form-group">
											<label>ORDEM DA AULA <b>*</b></label>
											<input type="text" class="form-control" placeholder="" id="ordem_aula"  required>
										</div>
									</div>
									<div class="col-md-6 px-1">
										<div class="form-group">
											<label>DESCRIÇÃO DA AULA <b>*</b></label>
											<input type="text" class="form-control" placeholder="" id="descricao_aula"  required>
										</div>
									</div>
									<div class="col-md-2 pl-1">
										<div class="form-group">
											<label for="exampleInputEmail1">MODULO</label>
											<select class="form-control show-tick" id="id_modulo" required>
												<option value="0" selected> Selecione </option>
												<% modulo.forEach(modulo=>{ %>
													<option value="<%=modulo.id_modulo %>"> <%=modulo.descricao %> </option>
												<% }) %>
											</select>
										</div>
									</div>
									<div class="col-md-4 pl-1">
										<div class="form-group">
											<label for="exampleInputEmail1">MATÉRIA</label>
											<select class="form-control show-tick" id="id_materia" required>
												<option value="0" selected> Selecione </option>
												<% materia.forEach(materia=>{ %>
													<option value="<%=materia.id_materia %>"> <%=materia.descricao %> </option>
												<% }) %>
											</select>
										</div>
									</div>
									<div class="col-md-12 pl-1">
										<div class="form-group">
											<label for="exampleInputEmail1">Link do Video</label>
											<textarea class="form-control" style="height: 100px" id="link"></textarea>
										</div>
									</div>
									
									<div class="col-md-4 pl-1" style="margin-top:30px;">
										<button type="submit" class="btn btn-primary btn-round" id="cadAula" style="padding-left: 15px;padding-right: 15px;padding-top: 5px;padding-bottom: 5px; font-size: 13px;">Cadastrar</button>
										<button type="reset" class="btn btn-warning btn-round"  style="padding-left: 15px;padding-right: 15px;padding-top: 5px;padding-bottom: 5px; font-size: 13px;">Limpar</button>
									</div>
								 
								</div>
						  </form>
