	<script>
        $(document).ready(function () {
			new DataTable('#example', {
				'paging'      : true,
				'lengthChange': false,
				'searching'   : true,
				'ordering'    : false,
				'info'        : true,
				'autoWidth'   : false
			});
			
			
			$("#cadAula").click(function(){
				let uid_usuario = $("#uid_usuario").val();
				//let ordem_aula ="1";
				let ordem_aula ="1";
				let descricao_aula=$("#descricao_aula").val();
				let id_materia=$("#id_materia").val();
				let id_modulo=$("#id_modulo").val();
				let link_video = $("#link").val();
				
				
				console.log(uid_usuario)
				console.log(ordem_aula)
				console.log(descricao_aula)
				console.log(id_materia)
				console.log(link_video)
				
				
				$.post('/adm/addAulas', {
					uid_usuario: uid_usuario,
					descricao: descricao_aula, 
					id_materia: id_materia, 
					id_modulo: id_modulo,
					link: link_video, 
					ordem_aula:ordem_aula
					
			
				}, function(resposta) {
					console.log(resposta)
					window.location.reload(true);
					
				});
				
			})  
        
          
		  
          function getCookie(name) {
              let cookies = document.cookie.split("; ");
              for (let i = 0; i < cookies.length; i++) {
                  let cookie = cookies[i];
                  let [cookieName, cookieValue] = cookie.split("=");
                  if (cookieName === name) {
                      return cookieValue;
                  }
              }
              return null;
          }
       
    
      })
      
      </script>
