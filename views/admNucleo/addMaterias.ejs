																<td style="width: 20%; text-align: center;"><%= dados.modulo %></td>
																<!--<td style="width: 5%;"><a href="/adm/remMaterias/<%= dados.id_materia %>/<%= uid_usuario %>"><span class="badge bg-danger" >Excluir</span></a></td>-->
